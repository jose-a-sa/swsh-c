{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) swsh",
            "type": "cppdbg",
            "request": "launch",
            "miDebuggerPath": "c:/msys64/mingw64/bin/gdb.exe",
            "program": "${workspaceRoot}/bin/swsh.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}